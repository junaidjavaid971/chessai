<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.com.chess.ai">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="app.com.chess.ai.views.activities.LevelsActivity"></activity>
        <activity android:name="app.com.chess.ai.views.activities.ChessboardActivity" />
        <activity android:name="app.com.chess.ai.views.activities.OptionsActivity" />
        <activity android:name="app.com.chess.ai.views.activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="app.com.chess.ai.views.activities.MainActivity" />
    </application>

</manifest>